{
    "manifest_version": 3,
    "name": "ScrollRot",
    "version": "1.0.0",
    "description": "Worsen the infinite scrolling experience",
    "icons": {
        "48": "images/icon-48.png",
        "128": "images/icon-128.png"
    },
    "action": {
        "default_popup": "index.html"
    },
    "permissions": [
        "scripting"
    ],
    "host_permissions": [
        "https://*/*",
        "http://*/*"
    ]
}